packet-generator new {
  name x
  limit 1
  node ip6-input
  size 64-64
  no-recycle
  data {
    ICMP6: 2000::2 -> 2001::2
    ICMP echo_request
    incrementing 100
  }
}

tr add pg-input 100
int create-ethernet
int create-ethernet
set int state fake-eth0 up
set int state fake-eth1 up

set int ip address fake-eth0 2000::1/64
set int ip address fake-eth1 2001::1/64
