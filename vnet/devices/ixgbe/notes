Intel BSD driver taxonomy:

ixgbe_attach()
        remember optics type
        ->ixgbe_identify_hardware()
        ->ixgbe_init_shared_code()
            ->ixgbe_set_mac_type()
            ->ixgbe_init_ops_XXX()
        ->ixgbe_hardware_init() see below
        ->ixgbe_allocate_msix() / ixgbe_allocate_legacy
        ->ixgbe_setup_interface()
        -> ixgbe_get_bus_info() - pci express bus setup
        set IXGBE_CTRL_EXT 

ixgbe_init_locked()
        ->ixgbe_stop()
        setup MAC address
        ->ixgbe_hardware_init()
            ->ixgbe_stop_adapter()
            setup flow-control setting
            ->ixgbe_init_hw()
                ->hw->mac.ops.init_hw()
                    ->ixgbe_init_hw_generic()
                        ->mac.ops.reset_hw() = ixgbe_init_ops_82598()
                            Power up TX lanes
                            ->hw->phy.ops.init()
                            ->hw->phy.ops.reset()
                        ->mac.ops.start_hw() = ixgbe_start_hw_82598()
                            ->ixgbe_start_hw_generic()
                                setup media type
                                ->hw.mac.ops.clear_vfta() vlan tbl
                                ->hw.mac.ops.clear_hw_cntrs()
                                set no snoop disable
                                ->ixgbe_setup_fc() flow control
                            disable relaxed ordering
                            ->ixgbe_set_pcie_completion_timeout()

        ->ixgbe_setup_transmit_structures()
        ->ixgbe_initialize_transmit_units()
        ->ixgbe_set_multi()
        ->ixgbe_setup_receive_structures()
        ->ixgbe_initialize_receive_units()
        ->ixgbe_init_moderation()
        setup GPIE's
        enable rx and TX queues
        ->ixgbe_setup_vlan_hw_support()
        enable receive engine
        ->ixgbe_config_link()
        ->ixgbe_enable_intr()
        ->hw->phy.ops.identify()

ixgbe_legacy_irq()
        read IXGBE_EICR
        link-status change (IXGBE_EICR_LSC)
            ->ixgbe_check_link()
            ->ixgbe_update_link_status()
        
ixgbe_setup_interface() mostly BSD except:
	->ifmedia                           
